<div class="action-snackbar">
    <Snackbar>
        <div class="snackbar-content">
            @Content
        </div>
        <div class="actions">
            @Actions
        </div>
    </Snackbar>
</div>
@code {
    [Parameter]
    public RenderFragment Content { get; set; } = @<div></div>;

    [Parameter]
    public RenderFragment Actions { get; set; } = @<div></div>;
}
